{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "build"
    },
    "exclude": [
        "node_modules",
        "config",
        "build",
        "release"
    ]
}